public class MinInArray{
    public static int minimum(int n[]){
        int smallest = Integer.MAX_VALUE;

        for(int i=1;i<n.length;i++){
            if(n[i]<smallest){
                smallest = n[i];
            }
        }
        return smallest;
    }
    public static void main(String args[]){
        int n[] = {7,3,8,5,9,10,4,1};

        System.out.print(minimum(n));

    }
}
